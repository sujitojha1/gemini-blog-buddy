{
  "manifest_version": 3,
  "name": "gemini-blog-buddy",
  "version": "1.0.0",
  "description": "Index packaged documents and search for relevant passages directly from the toolbar.",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Document Indexer",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  }
}
